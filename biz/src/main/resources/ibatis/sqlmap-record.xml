<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="record">
	
	<typeAlias alias="record" type="com.papa2.cube.api.record.bo.Record" />
	
	<select id="getRecordList" parameterClass="record" resultClass="record">
	<![CDATA[
		SELECT CONVERT(VARCHAR(100),t.InTime,20)  AS startTime,
		  CONVERT(     VARCHAR(100),t.OutTime,20) AS endTime,
		  t.CardNo                                AS parkCardNo,
		  t.BusNo                                 AS carNo
		FROM BusRecord t 
	]]>
	</select>
	
</sqlMap>